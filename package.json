  "homepage": "https://devflow.software/",
  "scripts": {
    "dev": "electron-vite dev",
    "start": "electron-vite preview",
    "dev:server": "npm --prefix src/server run dev",
    "start:server": "npm --prefix src/server run start",
    "build:server": "npm --prefix src/server run build",
    "prisma:generate": "npm --prefix src/server run prisma:generate",
    "prisma:migrate": "npm --prefix src/server run prisma:migrate",
    "prisma:studio": "npm --prefix src/server run prisma:studio",
    "sync:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "electron-vite build",
    "build:win": "npm run build ; electron-builder --win",
    "postgres:start": "net start postgresql-x64-15",
    "postgres:stop": "net stop postgresql-x64-15",
    "postgres:status": "sc query postgresql-x64-15"
  },
